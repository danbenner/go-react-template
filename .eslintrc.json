{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules":{
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": true, "peerDependencies": false}],
        "class-methods-use-this": ["error", { "exceptMethods": ["getKeyByValue"] }],
        "react/destructuring-assignment": 0,
        "react/prop-types": 0,
        "max-len": 0
    }
}